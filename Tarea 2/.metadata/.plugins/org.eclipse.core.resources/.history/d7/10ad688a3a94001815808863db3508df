PAPELILLO
	CUANTOSG I;
	CUANTOSG K ==~ HAYMANO;
	TENIMANO J;
	TENIMANO L;
	SEPA A;
	SEPA B;
	I ==~ 12;
	I ==~ K - I;
	K ==~ 12;
	B ==~ A;
	I ==~ K;
	J ==~ L;
	J ==~ HAYMANO;
	L ==~ NOHAYMANO;
	A ==~ HAYMANO;
	ENRROLAR(I K J L A); 
	ENRROLAR("fin del programa");
	
	CANNABIS(L <-> HAYMANO Y J >-< HAYMANO)
	{
		CUANTOSG P;
		J ==~NOHAYMANO;
		L ==~ HAYMANO;
		QUEMAR (P);
		ENRROLAR(P);
		
	}PARAGUA{
		CUANTOSG Q;
		SEPA H;
		H ==~ "hola";
		QUEMAR(H);
		ENRROLAR(Q);
	}

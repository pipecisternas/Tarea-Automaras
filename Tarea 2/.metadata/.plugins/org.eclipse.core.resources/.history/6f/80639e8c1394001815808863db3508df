PAPELILLO
	CUANTOSG I;
	CUANTOSG K;
	TENIMANO J;
	TENIMANO L;
	SEPA A;
	SEPA B;
	I ==~ 12;
	I ==~ K - I;
	K ==~ 12;
	B ==~ A;
	I ==~ K;
	J ==~ L;
	J ==~ HAYMANO;
	L ==~ NOHAYMANO;
	A ==~ "hola";
	ENRROLAR(I K J L A); 
	ENRROLAR("fin del programa");
	
	CANNABIS(L <-> HAYMANO Y J >-< HAYMANO)
	{
		CUANTOSG P;
		J ==~NOHAYMANO;
		L ==~ HAYMANO;
		QUEMAR (P);
		ENRROLAR(P);
		CANNABIS(L <-> HAYMANO Y J >-< HAYMANO O HAYMANO<->1)
		{
			CUANTOSG R;
			J ==~NOHAYMANO;
			L ==~ HAYMANO;
			QUEMAR (R);
			ENRROLAR(R);
		}PARAGUA{
			CUANTOSG M;
			SEPA W;
			W ==~ "hola";
			QUEMAR(W);
			ENRROLAR(M);
		}
	}PARAGUA{
		CUANTOSG Q;
		SEPA H;
		H ==~ "hola";
		QUEMAR(H);
		ENRROLAR(Q);
	}
